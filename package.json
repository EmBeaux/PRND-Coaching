{
  "name": "PRND-Coaching",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "build:framevuerk": "framevuerk-builder -c ./framevuerk-config.js",
    "prepublish": "npm run build:framevuerk",
    "test": "npm run test:unit && npm run test:e2e",
